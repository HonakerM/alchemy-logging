{
  "name": "alog",
  "version": "0.1.1",
  "description": "Alog implementation in NodeJS/TypeScript wrapping around the Bunyan logging framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "NODE_ENV=test nyc mocha --exit --require ts-node/register 'test/**/*.ts'",
    "lint": "./node_modules/tslint/bin/tslint --project . --config ./tslint.json",
    "clean": "rm -rf dist",
    "build": "tsc",
    "prepare": "npm run build",
    "preinstall": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.ibm.com:watson-nlu/alog-ts.git"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/**",
      "dist/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^12.12.5",
    "deepcopy": "^2.0.0",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "deep-equal": "^1.1.1",
    "memory-streams": "^0.1.3",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rewire": "^4.0.1",
    "ts-node": "^8.4.1",
    "tslint": "^6.0.0-beta0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-no-unused-expression-chai": "^0.1.4"
  }
}
